	TopBar.Position = UDim2.new(0.349772394, 0, 0.321739137, 0)
	TopBar.Size = UDim2.new(0, 396, 0, 30)
	TopBar.Active = true
	TopBar.Draggable = true

	UICorner.Parent = TopBar

	AssestTopBar.Name = "AssestTopBar"
