TitleScreen_2.TextScaled = true
TitleScreen_2.TextSize = 14.000
TitleScreen_2.TextWrapped = true
wait(5)
TitleScreen:Destroy()

function VisualLib:MakeWindow(WindowFile)
	WindowFile = WindowFile or {}
	WindowFile.Name = WindowFile.Name or ""
	local UiLibrary = Instance.new("ScreenGui")
	local TopBar = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local AssestTopBar = Instance.new("Frame")
	local Title = Instance.new("TextLabel")
	local UIFrame = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local DropShadowHolder = Instance.new("Frame")
	local DropShadow = Instance.new("ImageLabel")
	local TabContainer = Instance.new("Frame")
	local UICorner_4 = Instance.new("UICorner")
	local Cover = Instance.new("Frame")
	local Cover_2 = Instance.new("Frame")
	local TheTabContainer = Instance.new("Frame")
	local UIGridLayout = Instance.new("UIGridLayout")
	local ContainerFolder = Instance.new("Folder")
	
	UiLibrary.Name = "UiLibrary"
	UiLibrary.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	UiLibrary.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	TopBar.Name = "TopBar"
	TopBar.Parent = UiLibrary
